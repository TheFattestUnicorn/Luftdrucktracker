{
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-immutable"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [true, "dev"],
    "no-bitwise": [false],
    "no-console": {
      "severity": "warning"
    },
    "object-literal-sort-keys": [false]
  }
}
